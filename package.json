{
  "name": "cypress-typescript-tips-tricks",
  "version": "1.0.0",
  "description": "Project meant to discover new stuff that may help us to develop better scripts ",
  "main": "index.js",
  "scripts": {
    "test": "runner",
    "prettier": "npx prettier --write .",
    "lint": "npx eslint .",
    "prepare": "husky install",
    "runner-in-prod-env": "cypress open --env version=prod",
    "runner-in-test-env": "cypress open --env version=test",
    "runner-in-uat-env": "cypress open --env version=uat"
  },
  "keywords": [
    "cypress",
    "typescript"
  ],
  "author": "joan esquivel",
  "license": "ISC",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "cypress": "^12.8.1",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "prettier": "2.8.4",
    "typescript": "^5.0.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
